{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Server",
            "program": "${workspaceFolder}/server/server.ts",
            "preLaunchTask": "tsc: build - server/tsconfig.json",
            "outFiles": ["${workspaceFolder}/server/out/**/*.js"],
            "env":{
                "MYSQL_USER":"aUser",
                "MYSQL_PASSWORD":"aPassword",
                "MYSQL_DATABASE":"aDatabase"
            }
        }
    ]
}